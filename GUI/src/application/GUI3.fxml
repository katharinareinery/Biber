<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="650.0" prefWidth="800.0" styleClass="root" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.FXController2">
   <right>
      <VBox prefHeight="300.0" prefWidth="400.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fx:id="imageView3" fitHeight="400.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
               <VBox.margin>
                  <Insets left="20.0" right="50.0" top="50.0" />
               </VBox.margin></ImageView>
            <Button fx:id="buttonOriginalPic" mnemonicParsing="false" text="Original" textAlignment="JUSTIFY">
               <VBox.margin>
                  <Insets left="250.0" top="50.0" />
               </VBox.margin></Button>
         </children>
      </VBox>
   </right>
   <left>
      <VBox prefHeight="300.0" prefWidth="400.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fx:id="imageView2" disable="true" fitHeight="400.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
               <VBox.margin>
                  <Insets left="50.0" right="20.0" top="50.0" />
               </VBox.margin>
               <viewport>
                  <Rectangle2D />
               </viewport>
            </ImageView>
            <Button fx:id="buttonFilterPic" mnemonicParsing="false" text="Filter" textAlignment="JUSTIFY">
               <VBox.margin>
                  <Insets left="250.0" top="50.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </left>
</BorderPane>
